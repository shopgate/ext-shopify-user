{
  "version": "1.3.2-beta.3",
  "id": "@shopgate/shopify-user",
  "trusted": true,
  "steps": [
  ],
  "configuration": {
    "shopifyShopAlias": {
      "type": "bigApi",
      "destination": "backend",
      "default": null,
      "params": {
        "method": "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.frontend.alias"
      }
    },
    "shopifyAccessToken": {
      "type": "bigApi",
      "destination": "backend",
      "default": null,
      "params": {
        "method": "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.shopifyAccessToken"
      }
    },
    "getUserAccountSettings": {
      "type": "bigApi",
      "destination": "frontend",
      "default": "required",
      "params": {
        "method": "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/gui_sg_connect_user_account?parsed=true",
        "key": "value"
      }
    },
    "userRegistrationUrl": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": ""
      }
    },
    "userDataCacheTtl": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": 600000
      }
    },
    "stage": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": "%(targetStage)s"
      }
    },
    "credentials": {
      "type": "bigApiCredentials",
      "destination": "backend"
    },
    "requestTimeout": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": {
          "bigApi": 15000,
          "token": 2000
        }
      }
    }
  },
  "components": [
    {
      "id": "RegisterSubscription",
      "path": "frontend/Register/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "CheckoutSubscription",
      "path": "frontend/Checkout/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "GuestCheckoutLink",
      "path": "frontend/portals/GuestCheckoutLink/index.jsx",
      "target": "page.login.register-link.after",
      "type": "portals"
    },
    {
      "id": "GuestCheckoutLinkRoute",
      "path": "frontend/portals/GuestCheckoutLink/route.jsx",
      "target": "app.routes",
      "type": "portals"
    },
    {
      "id": "GuestCheckoutSubscription",
      "path": "frontend/portals/GuestCheckoutLink/subscribers.js",
      "type": "subscribers"
    },
    {
      "id": "locale/en-US",
      "type": "translations",
      "path": "frontend/locale/en-US.json"
    },
    {
      "id": "locale/de-DE",
      "type": "translations",
      "path": "frontend/locale/de-DE.json"
    },
    {
      "id": "locale/fr-FR",
      "type": "translations",
      "path": "frontend/locale/fr-FR.json"
    },
    {
      "id": "locale/nl-NL",
      "type": "translations",
      "path": "frontend/locale/nl-NL.json"
    }
  ]
}
